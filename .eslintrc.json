{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": 0,
        "implicit-arrow-linebreak": 0,
        "operator-linebreak": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "no-empty-function": "warn"
    }
}
